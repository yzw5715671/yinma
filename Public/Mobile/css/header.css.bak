/*Project/intro*/
#gallery img {max-width: 100%;}

.dynamic-comment {
	padding: 20px 30px;
	font-size: 14px;
	color: #666;
	line-height: 30px;
	position: relative;
}

.dynamic-comment h4.c2 {
	margin-top: 20px;
	margin-bottom: 20px;
}

#replyw {
	width: 100%;
}

#project-comment {
	box-sizing: border-box;
}

#btn-project-comment {
	right: 0;
}

.dynamic-section header, .dynamic-section .dynamic-content {
    width: 95%;
    margin: 0 auto;
    padding: 10px 0;
}
.dynamic-section header, .dynamic-comment-section header {
	background: inherit;
	text-align: left;
    height: auto;
    border-bottom: 1px solid #ddd;
}
.dynamic-section .proj-title {
    font-size: 20px;
    line-height: 1.5;
    margin: 10px 0;
    color: #555555;
}
.dynamic-section .proj-title a {
    font-size: inherit;
}
.dynamic-section .author-line {
    font: 16px/27px "微软雅黑";
    color: #8e8e8e;
}
.dynamic-section .dynamic-content {
    font: 16px/27px "微软雅黑";
    color: #555555;
}
.dynamic-section, .dynamic-comment-section{
    background: #fff;
    margin: 5px;
    border: 1px solid #dddddd;
}
.dynamic-comment-section {
    margin-bottom: 50px;
}
.dynamic-comment-section header {


}
.dynamic-comment-section header:after {
    content: '';
    display: block;
    position: absolute;
    width: 25px;
    height: 25px;
    right: 20px;
    top: 12px;
    background-image: url("../images/imgs/img79.png");
    -webkit-background-size: 25px 25px;
    -o-background-size: 25px 25px;
    background-size: 25px 25px;
}
.dynamic-comment-section.closed header:after {
    background-image: url('../images/imgs/img69.png');
}

.dynamic-comment-section h2 {
    text-align: left;
    color: #555555;
    margin-left: 3%;
}
.dynamic-comment-section #dynamic-comment-form {
    display: none;
}
.dynamic-comment-section .reply-form {
    display: none;
    margin-top: 20px;
}
.dynamic-comment-section .reply-form textarea {
    width: 100%;
    height: 5em;
    border: 1px solid #ddd;
    border-radius: 4px;
    font-size: 14px;
}
.dynamic-comment-section .button-post-reply, .dynamic-comment-section .button-slide {
    color: #858585;
    background: transparent;
    border: #a8a8a8 solid 1px;
    font-size: 14px;
    border-radius: 4px;
}
.dynamic-comment-section .button-post-reply {
    margin-top: 10px;
}

.dynamic-comment-section .avatar-box {
    display: inline-block;
    vertical-align: top;
    width: 20%;
    border-radius: 50%;
    overflow: hidden;
    margin-right: 15px;
    font-size: 0;
}
.dynamic-comment-section .avatar-img {
    max-width: 100%;
    max-height: 100%;
}
.dynamic-comment-section .dynamic-comment-item {
    margin: 15px 12px;
    padding-bottom: 15px;
    position: relative;
    border-bottom: 1px solid #f0f0f0;
}
.dynamic-comment-section .user-comment {
    display: inline-block;
    vertical-align: top;
    width: 70%;
    color: #5c5c5c;
}
.dynamic-comment-section  .quote-comment {
    background: #f2f2f2;
    padding: 10px;
    margin-top: 15px;
    color: #333;
    font-size: 14px;
    line-height: 24px;
}
.dynamic-comment-section .user-name {
    float: left;
    font-size: 18px;
    line-height: 2;
}
.dynamic-comment-section .comment-time {
    float: left;
    line-height: 36px;
    margin-left: 10px;
}
.dynamic-comment-section .comment-content {
    clear: both;
    margin-top: 5px;
}
.dynamic-comment-section .button-slide {
    position: absolute;
    top: 5px;
